train_net: "models/bodyhead/train.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
max_iter: 70000
stepsize: 30000
display: 10
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
snapshot: 10000
